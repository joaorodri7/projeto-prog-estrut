int main()
{
    int i;
    char sexo[15];
   
    
    printf("Qual o seu sexo? \nMasculino - Feminino - Nao identficar ");
    scanf("%s", sexo);

        if(strcmp(sexo, "masculino") == 0 || (strcmp(sexo, "feminino") == 0) || (strcmp(sexo, "NI") == 0)) 
        {
            printf("valor valido");
        }
